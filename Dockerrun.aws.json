{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "frontend",
        "image": "211125485899.dkr.ecr.us-west-2.amazonaws.com/cmpe272frontend:latest",
        "essential": true,
        "memory": 512,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": ["backend"],  
        "environment": [
          {
            "name": "REACT_APP_BACKEND_URL",
            "value": "http://localhost:8000"
          }
        ]
      },
      {
        "name": "backend",
        "image": "211125485899.dkr.ecr.us-west-2.amazonaws.com/cmpe272backend:latest",
        "essential": true,
        "memory": 512,
        "portMappings": [
          {
            "hostPort": 8080,
            "containerPort": 8080
          }
        ]
      }
    ]
  }
  